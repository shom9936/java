package generic;

class Box{
	
	private Object obj;

	public Object getObj() {
		return obj;
	}

	public void setObj(Object obj) {
		this.obj = obj;
	}
	
}

public class Ex02 {

	public static void main(String[] args) {
		
		Box box = new Box();
		box.setObj("안녕하세요");
		String str = (String)box.getObj();
		System.out.println(str);
		

	}

}
